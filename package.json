{
  "name": "solutions-wp4-es6-postcss-starter",
  "private": true,
  "version": "1.0.2",
  "description": "fork of Jonas Schmedtmann Natours project, remake with postcss & bem.",
  "repository": "https://github.com/wwwsolutions/solutions-wp4-es6-postcss-starter",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack",
    "gulp": "./node_modules/gulp/bin/gulp.js",
    "gulp:sprites": "gulp sprites",
    "gulp:modernizr": "gulp modernizr",
    "webpack-dev-server": "webpack-dev-server",
    "tunnel": "ngrok http --host-header=rewrite 8080",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "build": "webpack --env.mode production --config ./build-utils/webpack/webpack.common.babel.js --display-error-details",
    "watch": "webpack --env.mode development --watch --config ./build-utils/webpack/webpack.common.babel.js --display-error-details",
    "prod": "npm run webpack -- --config ./build-utils/webpack/webpack.common.babel.js --env.mode production",
    "prod:serve": "http-server dist/ -p 8888 -o",
    "prod:debug": "npm run debug -- --env.mode production",
    "prod:analyze": "npm run prod -- --env.presets analyze",
    "prod:compress": "npm run prod -- --env.presets compress --env.presets analyze",
    "dev": "npm run webpack-dev-server -- --config ./build-utils/webpack/webpack.common.babel.js --env.mode development",
    "dev:debug": "npm run debug -- --env.mode development"
  },
  "author": "Domagoj-Mario Mendas <www.programmingwww.solutions>",
  "license": "ISC",
  "keywords": [
    "Starter",
    "Boilerplate",
    "Webpack 4",
    "ES6",
    "Postcss"
  ],
