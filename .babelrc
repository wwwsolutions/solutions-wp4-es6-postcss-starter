{
  "presets": [
    ["@babel/preset-env", {
      "loose": true,
      "modules": false,
      "debug": true,
      "useBuiltIns": "usage",
        "targets": {
          "node": "current"
        }
    }]
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", {
      "corejs": false,
      "helpers": true,
      "regenerator": true,
      "useESModules": true
      }],
    ["@babel/plugin-transform-modules-commonjs"],
    ["@babel/plugin-syntax-dynamic-import"],
    ["@babel/plugin-transform-regenerator"],
    ["@babel/plugin-proposal-class-properties"],
  ]
}
